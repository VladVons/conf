auto lo
    iface lo inet loopback

auto eth0
    iface eth0 inet dhcp
    hwaddress ether a0:f3:c1:01:3f:68

auto eth1
    iface eth1 inet static
    address 192.168.2.2
    netmask 255.255.255.0
    gateway 192.168.2.1
    dns-nameservers 8.8.8.8


auto wlan0
iface wlan0 inet dhcp
    wpa-ssid R3-0976646510
    wpa-psk 197119822007

